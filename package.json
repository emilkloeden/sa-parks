{
  "name": "sa-parks",
  "version": "0.0.0-development",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "cover": "nyc npm t",
    "build": "npm-run-all --parallel build:*",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js",
    "build:main": "babel --copy-files --out-dir dist src",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "commit": "git-cz",
    "test": "mocha test --compilers js:babel-register",
    "test:watch": "npm t -w",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/emilkloeden/sa-parks.git"
  },
  "keywords": [
    "parks"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emilkloeden/sa-parks/issues"
  },
  "homepage": "https://github.com/emilkloeden/sa-parks#readme",
  "dependencies": {
    "unique-random-array": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "commitizen": "^2.9.5",
    "cz-conventional-changelog": "^1.2.0",
    "ghooks": "^2.0.0",
    "istanbul": "^0.4.5",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.1",
    "nyc": "^10.1.2",
    "rimraf": "^2.5.4",
    "semantic-release": "^6.3.2",
    "webpack": "^1.14.0"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
